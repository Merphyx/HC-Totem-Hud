{
  "schemaVersion": 1,
  "id": "hctotemhud",
  "version": "${version}",
  "name": "HC-Totem Hud",
  "description": "Zeigt dir die Aufladungen deiner Inventar-Totems und warnt dich, wenn die Aufladungen knapp werden.",
  "authors": ["Myronx"],
  "contact": {},
  "license": "MIT",
  "icon": "assets/hctotemhud/icon.png",
  "environment": "client",
  "entrypoints": {
    "modmenu": [
      "de.myronx.hctotemhud.config.DefaultConfig"
    ],
    "client": [
      "de.myronx.hctotemhud.HcTotemHud"
    ]
  },
  "mixins": [
    "hctotemhud.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "*"
  }
}
